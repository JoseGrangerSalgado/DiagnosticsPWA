{
  "name": "System ext",
  "description": "System reader extension",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {

  },
  "background": {
      "scripts":["./background.js"]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup" : "popup.html"
  },
  "externally_connectable": {
    "matches": [
                "*://localhost/*",
               "http://localhost:8000/PWA/"]
  },
  "permissions": [
      "activeTab",
      "<all_urls>",
      "system.cpu",
      "tabs"
  ]
  
  
}